{
  "name": "zanysoft/laravel-zip",
  "description": "Zip and Unzip files and directories with Laravel",
  "keywords": [
    "laravel-zip",
    "laravel",
    "laravel8",
    "laravel9",
    "zip",
    "unzip",
    "extract",
    "backup",
    "ZipArchive",
    "multiple",
    "merge"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergei Medvedev",
      "email": "serguei.medvedev@yandex.ru"
    }
  ],
  "support": {
      "email": "serguei.medvedev@yandex.ru",
      "issues": "https://github.com/sergeimedvedev/laravel-zip/issues"
  },
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "scrutinizer/ocular": "^1.9",
    "orchestra/testbench": "^6.24"
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\Zip\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanySoft\\Zip\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\Zip\\ZipServiceProvider"
      ],
      "aliases": {
        "Zip": "ZanySoft\\Zip\\Facades\\Zip"
      }
    }
  }
}
